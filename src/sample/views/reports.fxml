<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ReportsController">
   <children>
      <TextArea fx:id="textField" editable="false" layoutX="432.0" layoutY="48.0" prefHeight="181.0" prefWidth="218.0" />
      <Label layoutX="422.0" layoutY="14.0" text="Customer Appointments by Type and Month:" />
      <Button fx:id="closeBtn" layoutX="627.0" layoutY="461.0" mnemonicParsing="false" onAction="#onCloseBtn" text="Close" />
      <TableView fx:id="scheduleTable" layoutX="23.0" layoutY="315.0" prefHeight="171.0" prefWidth="597.0">
        <columns>
            <TableColumn fx:id="apptId" prefWidth="28.0" text="ID" />
            <TableColumn fx:id="apptTitle" prefWidth="67.0" text="Title" />
            <TableColumn fx:id="apptDescription" prefWidth="101.0" text="Description" />
            <TableColumn fx:id="apptType" prefWidth="85.0" text="Type" />
            <TableColumn fx:id="apptStart" prefWidth="120.0" text="Start" />
            <TableColumn fx:id="apptEnd" prefWidth="113.0" text="End" />
            <TableColumn fx:id="apptCustomerId" prefWidth="82.0" text="Customer ID" />
        </columns>
      </TableView>
      <ChoiceBox fx:id="contactBox" layoutX="23.0" layoutY="278.0" onAction="#onPull" prefWidth="150.0" />
   </children>
</AnchorPane>
